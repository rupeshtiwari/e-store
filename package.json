{
  "name": "e-store",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:modules": "ts-node ./build/index.ts",
    "build:prod": "ng build --environment=prod --base-href /e-store/",
    "g:delete": "git push origin --delete gh-pages",
    "g:add": "git add dist",
    "g:push": "git subtree push --prefix dist origin gh-pages",
    "g:publish": "npm run build:prod && npm run g:add && npm run g:push"
  },
  "private": true,
  "dependencies": {
